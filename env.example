# OpenAI API Key (optional - for AI-powered recommendations)
OPENAI_API_KEY=your_openai_api_key_here

# Application URL (for production)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Stadia Maps API Key (for map tiles)
# Get a free key from https://client.stadiamaps.com/signup/
NEXT_PUBLIC_STADIA_API_KEY=your_stadia_maps_api_key_here

# Web scraping settings
SCRAPING_TIMEOUT=10000
SCRAPING_CACHE_DURATION=86400000

# Development settings
NODE_ENV=development 